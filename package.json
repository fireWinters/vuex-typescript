{
  "name": "my-vue-vite",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "eslint": "eslint --fix --ext .js src/",
    "commit": "git-cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "vite": "^2.9.5",
    "eslint": "^7.27.0",
    "stylelint": "^13.13.1",
    "husky": "^7.0.0",
    "cz-customizable": "^6.3.0",
    "commitizen": "^4.2.3",
    "commitlint-config-cz": "^0.13.3",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "npm run eslint"
    ]
  }
}